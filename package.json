{
  "name": "electron-react-scaffold",
  "version": "0.0.1",
  "author": "tea",
  "private": true,
  "description": "react + electron 手脚架",
  "main": "main.js",
  "scripts": {
    "start": "concurrently --kill-others \"umi dev\" \"npm:watch:main\" \"npm:watch:electron\"",
    "start:electron": "npm run build:main && cross-env NODE_ENV=development electron .",
    "watch:electron": "nodemon --exec \"npm run start:electron\"",
    "watch:main": "tsc ./main.ts --watch",
    "template": "plop",
    "build": "npm run build:umi && npm run build:electron",
    "build:main": "tsc ./main.ts",
    "build:umi": "cross-env umi build && rimraf ./dist/*.js.map ./dist/*.css.map",
    "build:electron": "cross-env NODE_ENV=production tsc ./main.ts && electron-builder",
    "pack:electron": "cross-env NODE_ENV=production tsc ./main.ts && electron-builder --dir",
    "test": "umi test",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint-staged": "lint-staged",
    "lint-staged:all": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier-eslint --write 'src/**/*'",
    "release": "rimraf package-lock.json && standard-version -a && git push --follow-tags",
    "commit": "git add -A && git-cz"
  },
  "dependencies": {},
  "devDependencies": {
    "@ant-design/icons": "^4.1.0",
    "@ckeditor/ckeditor5-build-classic": "^15.0.0",
    "@ckeditor/ckeditor5-build-decoupled-document": "^16.0.0",
    "@ckeditor/ckeditor5-react": "^1.1.3",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@sentry/browser": "^5.15.5",
    "@sentry/webpack-plugin": "^1.11.1",
    "@types/classnames": "^2.2.9",
    "@types/crypto-js": "^3.1.44",
    "@types/jest": "^23.3.14",
    "@types/js-base64": "^2.3.1",
    "@types/js-md5": "^0.4.2",
    "@types/lodash": "^4.14.144",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^12.12.50",
    "@types/nprogress": "^0.2.0",
    "@types/numeral": "0.0.26",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-scrollbar": "^0.4.10",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-virtualized": "^9.21.4",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "@typescript-eslint/typescript-estree": "^2.10.0",
    "ag-grid-community": "^23.0.2",
    "ag-grid-react": "^23.0.3",
    "antd": "^4.2.0",
    "array-move": "^2.2.1",
    "async-validator": "^3.2.1",
    "axios": "^0.19.0",
    "babel-eslint": "^9.0.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.2.0",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^6.0.3",
    "crypto-js": "^4.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "js-base64": "^2.5.2",
    "js-md5": "^0.7.3",
    "lint-staged": "^7.2.2",
    "lodash-decorators": "^6.0.1",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "mockjs": "^1.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.4",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "plyr": "^3.5.6",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^4.1.0",
    "react-ga": "^2.7.0",
    "react-scrollbar": "^0.5.6",
    "react-sortable-hoc": "^1.10.1",
    "react-test-renderer": "^16.7.0",
    "react-use": "^12.2.2",
    "react-virtualized": "^9.21.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.2.0",
    "standard-version": "^8.0.2",
    "tea-eval": "0.0.6",
    "teaness": "^4.1.18",
    "typescript": "^3.7.3",
    "umi": "^2.12.6",
    "umi-plugin-react": "^1.8.0",
    "umi-types": "^0.2.0",
    "uuid": "^3.3.3"
  },
  "optionalDependencies": {
    "commitizen": "^4.1.2",
    "plop": "^2.6.0",
    "puppeteer": "^1.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier-eslint --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:all"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "build": {
    "productName": "demo",
    "appId": "tea.example.electron",
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    }
  }
}
